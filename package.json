{
  "name": "quiz",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://AdityaPratap006.github.io/React-Quiz-App",
  "dependencies": {
    "gh-pages": "^2.1.1",
    "node-sass": "^7.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.0"
  },
  "scripts": {
    "build:css": "postcss src/tailwind-styles/index.css -o src/index.css",
    "watch:css": "postcss src/tailwind-styles/index.css -o src/index.css -w",
    "start": "start npm run watch:css && react-scripts start",
    "build": "start npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "parallelshell": "^3.0.2",
    "postcss-cli": "^6.1.3",
    "tailwindcss": "^1.1.2"
  }
}
